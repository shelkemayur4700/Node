<div>
    <h1>Users List</h1>
    <table class="table">
        <thead>
            <th>id</th>
            <th>index</th>
            <th>phone</th>
        </thead>
        <tbody>
            {{#data}}
            <tr>
                <td><a href="/userprofile/:{{_id}}">{{_id}}</a>
                </td>
                <td>{{index}}</td>
                <td>{{phone}}</td>
            </tr>
            {{/data}}
        </tbody>
    </table>
</div>